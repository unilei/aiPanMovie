page{background-color:rgba(0,0,0,.9)}.actor{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:horizontal;-webkit-box-direction:normal;-webkit-flex-direction:row;flex-direction:row;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-flex-wrap:wrap;flex-wrap:wrap;width:96%;margin:20rpx auto 0}.actor-item{width:44%;border-radius:10rpx;background-color:#2b2b2b;padding:10rpx;margin-top:10rpx;margin-left:10rpx;border:1rpx solid grey;-webkit-box-shadow:0 0 10rpx 0rpx hsla(0,0%,100%,.2);box-shadow:0 0 10rpx 0rpx hsla(0,0%,100%,.2)}.actor-pic{width:200rpx;height:200rpx;border-radius:200rpx;overflow:hidden;margin:0 auto;text-align:center}.actor-pic image{width:100%}.actor-name{text-align:center;color:#fff;font-size:24rpx}.type-name{text-align:center;color:grey;font-size:24rpx}